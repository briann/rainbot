{
  "name": "rainbot",
  "version": "1.0.0",
  "main": "server.js",
  "dependencies": {
    "@google-cloud/datastore": "^1.4.1",
    "ejs": "^2.6.1",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "koa-views": "^6.1.4",
    "luxon": "^1.3.3",
    "node-fetch": "^2.2.0"
  },
  "devDependencies": {
    "@types/google-cloud__datastore": "^1.3.2",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-router": "^7.0.31",
    "@types/koa-views": "^2.0.3",
    "@types/luxon": "^1.2.2",
    "@types/node-fetch": "^2.1.2",
    "nodemon": "^1.18.3",
    "npm-check": "^5.7.1",
    "npm-run-all": "^4.1.3",
    "ts-node": "^7.0.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "build": "run-s build:compile build:copy-static",
    "build:compile": "tsc -p .",
    "build:copy-static": "cp -r src/views dist/",
    "clean": "rm -rf dist/",
    "deploy": "run-s clean build deploy:appengine",
    "deploy:appengine": "gcloud app deploy",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'ts-node' ./src/server.ts",
    "debug": "node --inspect --require ts-node/register src/server.ts"
  }
}
